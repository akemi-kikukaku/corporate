{
  "name": "kiku-to-kaku",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && next build && next-sitemap --config sitemap.config.js",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && next start",
    "lint": "next lint",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.6",
    "gray-matter": "^4.0.3",
    "marked": "11.2.0",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.0.0"
  },
  "devDependencies": {
    "@types/marked": "^5.0.0",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.21",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "^9.0.0",
    "next-sitemap": "^4.0.0",
    "prettier": "^3.0.0",
    "sass": "^1.38.1",
    "typescript": "5.2.2"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 70
  },
  "volta": {
    "node": "18.18.2",
    "yarn": "4.0.2"
  }
