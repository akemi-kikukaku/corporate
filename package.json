{
  "name": "kiku-to-kaku",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && next build && next-sitemap --config sitemap.config.js",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && next start",
    "lint": "next lint",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.6",
    "gray-matter": "^4.0.3",
    "marked": "^4.0.0",
    "next": "^11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intersection-observer": "^8.32.1"
  },
  "devDependencies": {
    "@types/marked": "^3.0.0",
    "@types/node": "^20.5.6",
    "@types/react": "^17.0.19",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.4",
    "eslint-config-prettier": "^8.3.0",
    "next-sitemap": "^1.6.203",
    "prettier": "^2.3.2",
    "sass": "^1.38.1",
    "typescript": "4.3.5"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
